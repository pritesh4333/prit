<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/pip_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    >


    <FrameLayout
        android:id="@+id/pipLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/bajaj_black"
        android:orientation="vertical"
        android:visibility="visible"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">


                <com.acumengroup.ui.textview.GreekTextView
                    android:id="@+id/nifty_txt"
                    style="?android:attr/progressBarStyleSmall"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="NA"
                    android:paddingLeft="@dimen/_10sdp"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_10sdp"

                    />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <com.acumengroup.ui.textview.GreekTextView
                        android:id="@+id/nifty_val_txt"
                        style="?android:attr/progressBarStyleSmall"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="left"
                        android:text="0.00"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_9sdp" />


                    <com.acumengroup.ui.textview.GreekTextView
                        android:id="@+id/nifty_chng_txt"
                        style="?android:attr/progressBarStyleSmall"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="left"
                        android:text="0.00"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_9sdp" />

                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">


                <com.acumengroup.ui.textview.GreekTextView
                    android:id="@+id/sensex_txt"
                    style="?android:attr/progressBarStyleSmall"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="NA"
                    android:paddingLeft="@dimen/_10sdp"
                    android:textColor="@color/white"

                    android:textSize="@dimen/_10sdp" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <com.acumengroup.ui.textview.GreekTextView
                        android:id="@+id/sensex_val_txt"
                        style="?android:attr/progressBarStyleSmall"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="left"
                        android:text="0.00"
                        android:textColor="@color/white"

                        android:textSize="@dimen/_9sdp" />

                    <com.acumengroup.ui.textview.GreekTextView
                        android:id="@+id/sensex_chng_txt"
                        style="?android:attr/progressBarStyleSmall"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="left"
                        android:text="0.00"
                        android:textColor="@color/white"

                        android:textSize="@dimen/_9sdp" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">


                <com.acumengroup.ui.textview.GreekTextView
                    android:id="@+id/fsymbol_txt"
                    style="?android:attr/progressBarStyleSmall"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/_10sdp"
                    android:layout_weight="1"
                    android:text="NA"
                    android:textColor="@color/white"
                    android:textSize="10sp" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <com.acumengroup.ui.textview.GreekTextView
                        android:id="@+id/fsymbol_val_txt"
                        style="?android:attr/progressBarStyleSmall"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="left"
                        android:text="0.00"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_9sdp" />

                    <com.acumengroup.ui.textview.GreekTextView
                        android:id="@+id/fsymbol_chng_txt"
                        style="?android:attr/progressBarStyleSmall"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="left"
                        android:text="0.00"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_9sdp" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:orientation="horizontal">

                <com.acumengroup.ui.textview.GreekTextView
                    android:id="@+id/secsymbol_txt"
                    style="?android:attr/progressBarStyleSmall"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"

                    android:layout_weight="1"
                    android:text="NA"
                    android:textColor="@color/white"
                    android:paddingLeft="@dimen/_10sdp"
                    android:textSize="10sp" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <com.acumengroup.ui.textview.GreekTextView
                        android:id="@+id/secsymbol_val_txt"
                        style="?android:attr/progressBarStyleSmall"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="left"
                        android:text="0.00"
                        android:textColor="@color/white"

                        android:textSize="@dimen/_9sdp" />

                    <com.acumengroup.ui.textview.GreekTextView
                        android:id="@+id/secsymbol_chng_txt"
                        style="?android:attr/progressBarStyleSmall"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="left"
                        android:text="0.00"
                        android:textColor="@color/white"

                        android:textSize="@dimen/_9sdp" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <com.acumengroup.ui.textview.GreekTextView
                    android:id="@+id/thrdsymbol_txt"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:paddingLeft="@dimen/_10sdp"
                    android:layout_weight="1"
                    android:text="NA"
                    android:textColor="@color/white"
                    android:textSize="10sp" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <com.acumengroup.ui.textview.GreekTextView
                        android:id="@+id/thrdsymbol_val_txt"

                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="left"
                        android:text="0.00"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_9sdp" />

                    <com.acumengroup.ui.textview.GreekTextView
                        android:id="@+id/thrdsymbol_chng_txt"

                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="left"
                        android:text="0.00"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_9sdp"/>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
    </FrameLayout>


</LinearLayout>