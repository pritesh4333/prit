<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/bg_drawable">

    <LinearLayout
        android:id="@+id/linearLayout_outer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/transparent"
        android:orientation="vertical"
        android:visibility="visible">

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/white" />

        <LinearLayout
            android:id="@+id/search_layout"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="@color/black"
            android:elevation="5dp"
            android:orientation="horizontal"
            android:visibility="visible"
            android:weightSum="1">

            <com.acumengroup.ui.button.GreekButton
                android:id="@+id/symbSearchBtn"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:background="@drawable/scrip_icon"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/symbSearchBtn1"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:background="@drawable/ripple_effect_view"
                android:paddingLeft="-10dp"
                app:srcCompat="@drawable/scrip_icon" />
        </LinearLayout>

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            app:cardCornerRadius="4dp"
            app:cardElevation="5dp">


            <LinearLayout
                android:id="@+id/symDetails_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/black"
                android:orientation="horizontal"
                android:visibility="gone">
<!--here-->
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <com.acumengroup.ui.textview.ScrollingTextView
                        android:id="@+id/symbol_name_text"
                        style="@style/BoldStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="5dp"
                        android:ellipsize="marquee"
                        android:singleLine="true"
                        android:text=" "
                        android:fontFamily="@string/GREEK_TEXTVIEW_FONT"
                        android:textColor="@color/white"
                        android:textSize="14sp" />

                    <com.acumengroup.ui.textview.ScrollingTextView
                        android:id="@+id/symbols_description_text"
                        style="@style/BoldStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="5dp"
                        android:ellipsize="marquee"
                        android:singleLine="true"
                        android:fontFamily="@string/GREEK_TEXTVIEW_FONT"
                        android:text="HDFC"
                        android:textColor="@color/white"
                        android:textSize="14sp"
                        android:visibility="gone" />

                    <com.acumengroup.ui.textview.GreekTextView
                        android:id="@+id/symbols_exchange"
                        style="@style/BoldStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="5dp"
                        android:ellipsize="marquee"
                        android:singleLine="true"
                        android:textColor="@color/white"
                        android:textSize="14sp" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:weightSum="2">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="3dp"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <com.acumengroup.ui.textview.GreekTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="10dp"
                                android:layout_marginTop="2dp"
                                android:text="Bid"
                                android:textColor="@color/white"
                                android:textSize="14sp" />

                            <com.acumengroup.ui.textview.GreekTextView
                                android:id="@+id/txt_bid"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="10dp"
                                android:layout_marginTop="3dp"
                                android:text="00.00"
                                android:textColor="@color/white"
                                android:textSize="14sp" />




                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="3dp"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <com.acumengroup.ui.textview.GreekTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="10dp"
                                android:layout_marginTop="2dp"
                                android:text="Ask"
                                android:textColor="@color/white"
                                android:textSize="14sp" />

                            <com.acumengroup.ui.textview.GreekTextView
                                android:id="@+id/txt_ask"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="10dp"
                                android:layout_marginTop="3dp"
                                android:text="00.00"
                                android:textColor="@color/white"
                                android:textSize="14sp" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <com.acumengroup.ui.textview.GreekTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="2dp"
                            android:text="Limit"
                            android:textColor="@color/white"
                            android:textSize="14sp" />

                        <com.acumengroup.ui.textview.GreekTextView
                            android:id="@+id/txt_limit"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="3dp"
                            android:text="00.00"
                            android:textColor="@color/white"
                            android:textSize="14sp" />
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <com.acumengroup.ui.textview.GreekTextView
                        android:id="@+id/last_price_text"
                        style="@style/BoldStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="end"
                        android:layout_marginRight="5dp"
                        android:text="00.00"
                        android:textColor="@color/white"
                        android:textSize="14sp" />

                    <LinearLayout
                        android:id="@+id/streamingValues"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="end"
                        android:layout_marginRight="5dp"
                        android:orientation="horizontal"
                        android:visibility="visible">


                        <ImageView
                            android:id="@+id/imgChange"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/ic_up" />

                        <com.acumengroup.ui.textview.GreekTextView
                            android:id="@+id/change_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:text="00.00 (00.00%)"
                            android:textColor="@color/white"
                            android:textSize="13sp" />


                    </LinearLayout>

                    <com.acumengroup.ui.textview.GreekTextView
                        android:id="@+id/last_time_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="end"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:text="00/00/0000"
                        android:textColor="#FFFFFF"
                        android:textSize="12sp" />

                    <com.acumengroup.ui.button.GreekButton
                        android:id="@+id/best5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="end"
                        android:layout_marginTop="5dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginBottom="5dp"
                        android:background="@drawable/button_style"
                        android:text="Mkt. Depth" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="end"
                        android:paddingRight="15dp"
                        android:paddingBottom="5dp"
                        android:orientation="horizontal">
                        <com.acumengroup.ui.textview.GreekTextView
                            android:id="@+id/holding_lable"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="2dp"
                            android:text="Holding: "
                            android:textColor="@color/white"
                            android:textSize="14sp" />

                        <com.acumengroup.ui.textview.GreekTextView
                            android:id="@+id/holding_txt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="3dp"
                            android:text="00"
                            android:textColor="@color/white"
                            android:textSize="14sp" />
                    </LinearLayout>


                </LinearLayout>

            </LinearLayout>

        </androidx.cardview.widget.CardView>


        <RelativeLayout
            android:id="@+id/symDetails_layout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone">

            <com.acumengroup.ui.button.GreekButton
                android:id="@+id/best5_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_margin="10dp"
                android:background="@color/red_textcolor"
                android:padding="6dp"
                android:singleLine="true"
                android:text="@string/TD_BEST5_BTN"
                android:textSize="12sp" />

            <!--
                           &lt;!&ndash; <arcadian.GreekButton
                                android:id="@+id/buttonDemo_Test"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                               android:layout_toLeftOf="@id/best5"
                                android:layout_toStartOf="@id/best5"
                                android:layout_margin="10dp"
                                android:background="@color/trade_but_sel"
                                android:padding="6dp"
                                android:singleLine="true"
                                android:text="Demo"
                                android:visibility="visible"
                                android:textSize="12sp"/>&ndash;&gt;-->
            <Spinner
                android:id="@+id/exchange_spinner"
                android:layout_width="100dp"
                android:layout_height="28dp"
                android:layout_marginTop="10dp"
                android:nestedScrollingEnabled="true"
                android:singleLine="true"
                android:visibility="gone" />
            <!--android:layout_toLeftOf="@id/best5"-->


            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_toStartOf="@+id/exchange_spinner"
                android:layout_toLeftOf="@+id/exchange_spinner"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical">

                    <com.acumengroup.ui.textview.ScrollingTextView
                        android:id="@+id/symbol_name_text1"
                        style="@style/BoldStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:ellipsize="marquee"
                        android:fontFamily="@string/GREEK_TEXTVIEW_FONT"
                        android:singleLine="true"
                        android:textColor="@color/black"
                        android:textSize="14sp" />

                    <com.acumengroup.ui.textview.GreekTextView
                        android:id="@+id/last_price_text1"
                        style="@style/BoldStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="end"
                        android:textColor="@color/black"
                        android:textSize="14sp" />
                </LinearLayout>

            </LinearLayout>
        </RelativeLayout>


        <View
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#ffffff" />

        <Switch
            android:id="@+id/buySellswitch"
            android:layout_width="105dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@drawable/custom_switch"
            android:textColor="@color/black"
            android:textOff="Sell"
            android:textOn="Buy"
            android:visibility="gone"
            tools:targetApi="ice_cream_sandwich" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="5dp"
            android:background="@drawable/custom_switch">
            <!-- <RadioGroup
                    android:id="@+id/btnGroup"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="4dp"
                    android:visibility="visible"
                    android:background="@drawable/custom_switch">-->

            <RadioGroup
                android:id="@+id/btnGroup"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <RadioButton
                    android:id="@+id/btnBuy"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ripple_radio_button"
                    android:button="@null"
                    android:gravity="center"
                    android:padding="8dp"
                    android:singleLine="true"
                    android:text="@string/TD_BUY_BTN"
                    android:fontFamily="@string/GREEK_TEXTVIEW_FONT"
                    android:textColor="@color/white"
                    android:textSize="12sp" />

                <RadioButton
                    android:id="@+id/btnSell"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ripple_radio_button_sell"
                    android:button="@null"
                    android:fontFamily="@string/GREEK_TEXTVIEW_FONT"
                    android:gravity="center"
                    android:padding="8dp"
                    android:singleLine="true"
                    android:text="@string/TD_SELL_BTN"
                    android:textColor="@color/white"
                    android:textSize="12sp" />

                <!-- &lt;!&ndash;<RadioButton
                     android:id="@+id/btnMBuy"
                     android:layout_width="0dp"
                     android:layout_height="wrap_content"
                     android:layout_margin="5dp"
                     android:layout_weight="1.3"
                     android:background="@drawable/trade_radio_bg"
                     android:button="@null"
                     android:gravity="center"
                     android:padding="8dp"
                     android:singleLine="true"
                     android:text="@string/TD_MBUY_BTN"
                     android:textSize="12sp"/>&ndash;&gt;

                 &lt;!&ndash;<RadioButton
                     android:id="@+id/btnMSell"
                     android:layout_width="0dp"
                     android:layout_height="wrap_content"
                     android:layout_margin="5dp"
                     android:layout_weight="1.3"
                     android:background="@drawable/trade_radio_bg"
                     android:button="@null"
                     android:gravity="center"
                     android:padding="8dp"
                     android:singleLine="true"
                     android:text="@string/TD_MSELL_BTN"
                     android:textSize="12sp"/>&ndash;&gt;-->
            </RadioGroup>
        </LinearLayout>
        <!--<View
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#CCCCCC"/>-->

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="5dp"
                android:gravity="center"
                android:orientation="horizontal"
                android:weightSum="3">

                <Spinner
                    android:id="@+id/productType_Spinner"
                    android:layout_width="0dp"
                    android:layout_height="35dp"
                    android:layout_gravity="center"
                    android:layout_margin="5dp"
                    android:layout_weight="1"
                    android:nestedScrollingEnabled="true"
                    android:singleLine="true" />

                <Spinner
                    android:id="@+id/orderType_Spinner"
                    android:layout_width="0dp"
                    android:layout_height="35dp"
                    android:layout_gravity="center"
                    android:layout_margin="5dp"
                    android:layout_weight="1"
                    android:nestedScrollingEnabled="true"

                    android:singleLine="false" />

                <Spinner
                    android:id="@+id/orderLife_Spinner"
                    android:layout_width="0dp"
                    android:layout_height="35dp"
                    android:layout_margin="5dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:nestedScrollingEnabled="true"
                    android:singleLine="false" />

            </LinearLayout>

            <RelativeLayout
                android:id="@+id/daySpinnerLay"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">
                <!--
                                <View
                                    android:layout_width="wrap_content"
                                    android:layout_height="0.5dp"
                                    android:layout_centerInParent="true"
                                    android:background="#E4E4E4"/>-->


            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <com.acumengroup.ui.textview.GreekTextView
                        android:id="@+id/qty_label"
                        android:layout_width="140dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="2dp"
                        android:layout_marginRight="5dp"
                        android:layout_weight="1"
                        android:text="Quantity"
                        android:textColor="@color/white" />

                    <com.acumengroup.ui.edittext.GreekEditText
                        android:id="@+id/qty_text"
                        android:layout_width="140dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="2dp"
                        android:layout_marginRight="5dp"
                        android:layout_weight="1"
                        android:inputType="number"
                        android:focusable="true"
                        android:maxLength="8"
                        android:singleLine="true"
                        android:textColor="@color/white" />


                </LinearLayout>


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <com.acumengroup.ui.textview.GreekTextView
                        android:id="@+id/priceLbl"
                        android:layout_width="140dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="2dp"
                        android:layout_marginRight="5dp"
                        android:layout_weight="1"
                        android:text="Price"
                        android:textColor="@color/white" />

                    <com.acumengroup.ui.edittext.GreekEditText
                        android:id="@+id/price_text"
                        android:layout_width="140dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="2dp"
                        android:layout_marginRight="5dp"
                        android:layout_weight="1"
                        android:inputType="numberDecimal"
                        android:maxLength="13"
                        android:singleLine="true"
                        android:textColor="@color/white"
                        android:textColorHint="@color/white" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <com.acumengroup.ui.textview.GreekTextView
                        android:id="@+id/value_label"
                        android:layout_width="140dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="2dp"
                        android:layout_marginRight="5dp"
                        android:layout_weight="1"
                        android:text="Value"
                        android:textColor="@color/white" />

                    <com.acumengroup.ui.edittext.GreekEditText
                        android:id="@+id/value_text"
                        android:layout_width="140dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="2dp"
                        android:layout_marginRight="5dp"
                        android:layout_weight="1"
                        android:inputType="numberDecimal"
                        android:focusable="true"
                        android:singleLine="true"
                        android:textColor="@color/white" />


                </LinearLayout>


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:visibility="gone">

                    <com.acumengroup.ui.textview.GreekTextView
                        android:layout_width="140dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="2dp"
                        android:layout_marginRight="5dp"
                        android:layout_weight="1"
                        android:text="Limit Price"
                        android:textColor="@color/white" />

                    <com.acumengroup.ui.edittext.GreekEditText
                        android:id="@+id/limit_price_text"
                        android:layout_width="140dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="2dp"
                        android:layout_marginRight="5dp"
                        android:layout_weight="1"
                        android:hint="@string/TD_SL_LIMIT_PRICE_HINT_TXT"
                        android:inputType="numberDecimal"
                        android:maxLength="13"
                        android:singleLine="true"
                        android:textColor="@color/white"
                        android:textColorHint="@color/white"
                        android:visibility="visible" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <com.acumengroup.ui.textview.GreekTextView
                        android:id="@+id/triggerPriceLbl"
                        android:layout_width="140dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="2dp"
                        android:layout_marginRight="5dp"
                        android:layout_weight="1"
                        android:text="Trigger Price"
                        android:textColor="@color/white" />

                    <com.acumengroup.ui.edittext.GreekEditText
                        android:id="@+id/trig_price_text"
                        android:layout_width="140dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="2dp"
                        android:layout_marginRight="5dp"
                        android:layout_weight="1"
                        android:hint=""
                        android:inputType="numberDecimal"
                        android:maxLength="13"
                        android:singleLine="true"
                        android:textColor="@color/white"
                        android:textColorHint="@color/white" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <com.acumengroup.ui.textview.GreekTextView
                        android:id="@+id/disQty_label"
                        android:layout_width="140dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="2dp"
                        android:layout_marginRight="5dp"
                        android:layout_weight="1"
                        android:text="Disclosed Qty"
                        android:textColor="@color/white" />

                    <com.acumengroup.ui.edittext.GreekEditText
                        android:id="@+id/disc_qty_text"
                        android:layout_width="140dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="2dp"
                        android:layout_marginRight="5dp"
                        android:layout_weight="1"
                        android:hint=""
                        android:inputType="number"
                        android:maxLength="8"
                        android:singleLine="true"
                        android:textColor="@color/white"
                        android:textColorHint="@color/white" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/gtd_expiry_layout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:visibility="gone">

                    <com.acumengroup.ui.textview.GreekTextView
                        android:id="@+id/gtd_label"
                        android:layout_width="140dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="2dp"
                        android:layout_marginRight="5dp"
                        android:layout_weight="1"
                        android:text="Gtd Expiry"
                        android:textColor="@color/white" />

                    <com.acumengroup.ui.edittext.GreekEditText
                        android:id="@+id/gtd_text"
                        android:layout_width="140dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="2dp"
                        android:layout_marginRight="5dp"
