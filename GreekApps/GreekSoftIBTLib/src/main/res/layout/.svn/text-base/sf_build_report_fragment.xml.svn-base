<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/report_layout"
    android:background="@color/bajaj_black"
    >

    <LinearLayout
        android:id="@+id/header_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:background="@color/bajaj_gray"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="1dp"
                android:layout_weight="1"
                android:padding="3dp"
                android:orientation="vertical">

                <com.acumengroup.ui.textview.GreekTextView
                    android:id="@+id/mid_strike_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:text="Mid Strike"
                    android:textColor="@color/white">

                </com.acumengroup.ui.textview.GreekTextView>

                <com.acumengroup.ui.textview.GreekTextView
                    android:id="@+id/mid_strike"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_20sdp"
                    android:textAlignment="center"
                    android:background="@color/bajaj_black"
                    android:layout_margin="2dp"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:text="0.0"
                    android:textColor="@color/white">

                </com.acumengroup.ui.textview.GreekTextView>
            </LinearLayout>


            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="1dp"
                android:layout_weight="1"
                android:padding="3dp"
                android:orientation="vertical">

                <com.acumengroup.ui.textview.GreekTextView
                    android:id="@+id/mid_strikediff_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:text="Strike Diff"
                    android:textColor="@color/white">

                </com.acumengroup.ui.textview.GreekTextView>

                <com.acumengroup.ui.textview.GreekTextView
                    android:id="@+id/strike_diff"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_20sdp"
                    android:textAlignment="center"
                    android:background="@color/bajaj_black"
                    android:layout_margin="2dp"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:text="0.0"
                    android:textColor="@color/white"></com.acumengroup.ui.textview.GreekTextView>
            </LinearLayout>



            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="1dp"
                android:layout_weight="1"
                android:padding="3dp"
                android:orientation="vertical">

                <com.acumengroup.ui.textview.GreekTextView
                    android:id="@+id/mid_strikerange_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:text="Range Up"
                    android:textColor="@color/white">

                </com.acumengroup.ui.textview.GreekTextView>

                <com.acumengroup.ui.edittext.GreekEditText
                    android:id="@+id/range_up"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_20sdp"
                    android:textAlignment="center"
                    android:background="@color/bajaj_black"
                    android:layout_margin="2dp"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:text="0"
                    android:inputType="number"
                    android:textColor="@color/white"></com.acumengroup.ui.edittext.GreekEditText>
            </LinearLayout>




            <LinearLayout
