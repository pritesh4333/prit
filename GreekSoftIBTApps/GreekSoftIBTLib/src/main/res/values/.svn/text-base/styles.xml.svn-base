    <style name="radioButtonStyle" parent="@android:style/Widget.CompoundButton.RadioButton">
        <item name="android:button">@drawable/greek_radio</item>
    </style>
    <style name="SCBSwitch" parent="Theme.AppCompat.Light">
        <!-- active thumb & track color (30% transparency) -->
        <item name="colorControlActivated">@color/common_red_bg</item>

        <!-- inactive thumb color -->
        <item name="colorSwitchThumbNormal">@color/common_red_bg</item>

        <!-- inactive track color (30% transparency) -->
        <item name="android:colorForeground">@color/light_red_negative</item>
    </style>
    <!-- The background color of the Bottomsheet -->
    <style name="CustomTextView" parent="@android:style/Widget.TextView">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:padding">10dp</item>
    </style>
    <!-- The text appearance of the title -->
    <style name="DrawerItem" parent="@android:style/TextAppearance">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:singleLine">true</item>
        <item name="android:padding">10dp</item>
    </style>
    <!-- The text appearance of the list items -->
    <style name="DrawerItem.Child">
        <item name="android:textSize">13sp</item>
    </style>
    <!-- The text appearance of the grid items -->
    <style name="ListItem" parent="@android:style/TextAppearance">
        <item name="android:singleLine">true</item>
        <item name="android:padding">2dp</item>
    </style>
    <!-- The text appearance of the message -->
    <style name="ListItem.Head">
        <item name="android:textColor">@color/list_head_row_color</item>
        <item name="android:textSize">@dimen/list_subhead_text_size</item>
    </style>
    <!-- The text appearance of the title for a message Bottomsheet-->
    <style name="ListItem.Head.Bold">
        <item name="android:textStyle">bold</item>
    </style>
    <!-- The text appearance of the buttons -->
    <style name="ListItem.Head.First">
        <item name="android:marqueeRepeatLimit">2</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:textStyle">bold</item>
    </style>
    <!-- The color to tint the Bottomsheet icon -->
    <style name="ListItem.Head.Values">
        <item name="android:ellipsize">end</item>
        <item name="android:textStyle">bold</item>
    </style>
    <!-- The spacing between grid items -->
    <style name="TextLabelblack" parent="TextAppearance.AppCompat">
        <!-- Hint color and label color in FALSE state -->
        <item name="android:textColorHint">@color/black</item>
        <!-- Label color in TRUE state and bar color FALSE and TRUE State -->
    </style>
    <!-- The bottom padding of the grid -->
    <style name="TextLabelwhite" parent="TextAppearance.AppCompat">
        <!-- Hint color and label color in FALSE state -->
        <item name="android:textColorHint">@color/white</item>
        <!-- Label color in TRUE state and bar color FALSE and TRUE State -->
    </style>
    <!-- The top padding of the grid -->
    <style name="ListItem.SubHead">
        <item name="android:textColor">@color/list_subhead_row_color</item>
        <item name="android:textSize">@dimen/list_subhead_text_size</item>
    </style>
    <!-- The selector to be used for the items in the list/grid -->
    <style name="StyledIndicators" parent="@android:style/Theme.Light">
        <item name="vpiCirclePageIndicatorStyle">@style/CustomCirclePageIndicator</item>
        <item name="vpiLinePageIndicatorStyle">@style/CustomLinePageIndicator</item>
        <item name="vpiTitlePageIndicatorStyle">@style/CustomTitlePageIndicator</item>
        <item name="vpiTabPageIndicatorStyle">@style/CustomTabPageIndicator</item>
        <item name="vpiUnderlinePageIndicatorStyle">@style/CustomUnderlinePageIndicator</item>
    </style>
    <!-- The number of columns to show when using the grid style -->
    <style name="CustomCirclePageIndicator">
        <item name="fillColor">#FF888888</item>
        <item name="strokeColor">#FF000000</item>
        <item name="strokeWidth">2dp</item>
        <item name="radius1">10dp</item>
        <item name="centered">true</item>
    </style>


