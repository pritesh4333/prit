<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/_40sdp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/parent_layout"
    android:orientation="horizontal">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <View
            android:id="@+id/view1"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            app:layout_constraintTop_toTopOf="@+id/layout1"
            android:background="@color/white" />

        <LinearLayout
            android:id="@+id/layout1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">


            <com.acumengroup.ui.textview.GreekTextView
                android:id="@+id/dscript_name"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="2"
                android:gravity="start"
                android:text="SCRIP Nanme"
                android:textColor="@color/white">

            </com.acumengroup.ui.textview.GreekTextView>

            <View
                android:id="@+id/view11"
                android:layout_width="0.5dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_2sdp"
                android:layout_marginRight="@dimen/_2sdp"
                android:background="@color/white" />

            <com.acumengroup.ui.textview.GreekTextView
                android:id="@+id/dfree_qty"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="end"
                android:text="Free QTY"
                android:textColor="@color/white">

            </com.acumengroup.ui.textview.GreekTextView>

            <View
                android:id="@+id/view2"
                android:layout_width="0.5dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_2sdp"
                android:layout_marginRight="@dimen/_2sdp"
                android:background="@color/white" />


            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="2">

                <androidx.appcompat.widget.AppCompatCheckBox
                    android:id="@+id/authrize_checkbox"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_margin="@dimen/_2sdp"
                    android:layout_weight="1"
                    android:button="@drawable/checkbox_selector"
                    android:checked="true"
                    android:gravity="center" />

                <com.acumengroup.ui.edittext.GreekEditText
                    android:id="@+id/dsell_market"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/_3sdp"
                    android:layout_weight="2"
                    android:backgroundTint="@color/white"
                    android:digits="0123456789"
                    android:gravity="center"
                    android:imeOptions="actionDone"
                    android:inputType="number"
                    android:maxLength="10"
                    android:text="20"
                    android:textColor="@color/white" />

            </LinearLayout>

            <View
                android:id="@+id/view3"
                android:layout_width="0.5dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_2sdp"
                android:layout_marginRight="@dimen/_2sdp"
                android:background="@color/white" />

            <com.acumengroup.ui.textview.GreekTextView
                android:id="@+id/dtotal_qty"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="end"
                android:text="Total QTY"
                android:textColor="@color/white">

            </com.acumengroup.ui.textview.GreekTextView>

            <View
                android:id="@+id/view4"
                android:layout_width="0.5dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_2sdp"
                android:layout_marginRight="@dimen/_2sdp"
                android:background="@color/white" />

            <com.acumengroup.ui.textview.GreekTextView
                android:id="@+id/drate"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="end"
                android:text="Rate"
                android:textColor="@color/white">

            </com.acumengroup.ui.textview.GreekTextView>

            <View
                android:id="@+id/view5"
                android:layout_width="0.5dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_2sdp"
                android:layout_marginRight="@dimen/_2sdp"
                android:background="@color/white" />

            <com.acumengroup.ui.textview.GreekTextView
                android:id="@+id/dvalue"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="end"
                android:text="Value"
                android:textColor="@color/white">

            </com.acumengroup.ui.textview.GreekTextView>

            <View
                android:id="@+id/view6"
                android:layout_width="0.5dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_2sdp"
                android:layout_marginRight="@dimen/_2sdp"
                android:background="@color/white" />

            <com.acumengroup.ui.textview.GreekTextView
                android:id="@+id/dtoday_sell"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="end"
                android:text="Today Sell marked"
                android:textColor="@color/white">

            </com.acumengroup.ui.textview.GreekTextView>

            <View
                android:id="@+id/view7"
                android:layout_width="0.5dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_2sdp"
                android:layout_marginRight="@dimen/_2sdp"
                android:background="@color/white" />

            <com.acumengroup.ui.textview.GreekTextView
                android:id="@+id/dtotal_sell"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="end"
                android:text="Total Sell marked"
                android:textColor="@color/white">

            </com.acumengroup.ui.textview.GreekTextView>

            <View
                android:id="@+id/view8"
                android:layout_width="0.5dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_2sdp"
                android:layout_marginRight="@dimen/_2sdp"
                android:background="@color/white" />

            <com.acumengroup.ui.textview.GreekTextView
                android:id="@+id/dbalance_qty"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="end"
                android:text="Balance qty  "
                android:textColor="@color/white">

            </com.acumengroup.ui.textview.GreekTextView>

            <View
                android:id="@+id/view9"
                android:layout_width="0.5dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_2sdp"
                android:layout_marginRight="@dimen/_2sdp"
                android:background="@color/white" />

            <com.acumengroup.ui.textview.GreekTextView
                android:id="@+id/dtoday_sold"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="end"
                android:text="Today Sold Qty "
                android:textColor="@color/white">

            </com.acumengroup.ui.textview.GreekTextView>

        </LinearLayout>

        <View
            android:id="@+id/view10"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            app:layout_constraintBottom_toBottomOf="@+id/layout1"
            android:background="@color/white" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</LinearLayout>