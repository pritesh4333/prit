<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:padding="@dimen/_5sdp">

            <com.acumengroup.ui.textview.GreekTextView
                android:id="@+id/txt_script"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Script Name"
                android:textColor="@color/white"
                android:textSize="@dimen/_10sdp" />


            <com.acumengroup.ui.textview.GreekTextView
                android:id="@+id/txt_change"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Change(%)"
                android:textColor="@color/white"
                android:textSize="@dimen/_9sdp" />


        </LinearLayout>

        <View
            android:layout_width="0.5dp"
            android:layout_height="match_parent"
            android:background="@color/modify_order_border" />

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:padding="@dimen/_5sdp">

            <com.acumengroup.ui.textview.GreekTextView
                android:id="@+id/txt_quantity"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Quantity"
                android:textColor="@color/white"
                android:textSize="@dimen/_10sdp" />


            <com.acumengroup.ui.textview.GreekTextView
                android:id="@+id/txt_ltp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="CMP"
                android:textColor="@color/white"
                android:textSize="@dimen/_9sdp" />


        </LinearLayout>

        <View
            android:layout_width="0.5dp"
            android:layout_height="match_parent"
            android:background="@color/modify_order_border" />

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:padding="@dimen/_5sdp">

            <com.acumengroup.ui.textview.GreekTextView
                android:id="@+id/txt_inv_Price"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Inv. Val"
                android:textColor="@color/white"
                android:textSize="@dimen/_10sdp" />

            <com.acumengroup.ui.textview.GreekTextView
                android:id="@+id/txt_price"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Avg. Price"
                android:textColor="@color/white"
                android:textSize="@dimen/_9sdp" />


        </LinearLayout>

        <View
            android:layout_width="0.5dp"
            android:layout_height="match_parent"
            android:background="@color/modify_order_border" />

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:padding="@dimen/_5sdp">

            <com.acumengroup.ui.textview.GreekTextView
                android:id="@+id/txt_cPrice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Cur. Price"
                android:textColor="@color/white"
                android:textSize="@dimen/_10sdp" />

            <com.acumengroup.ui.textview.GreekTextView
                android:id="@+id/txt_mtfFund"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Closing Rate"
                android:textColor="@color/white"
                android:textSize="@dimen/_9sdp" />


        </LinearLayout>


        <View
            android:id="@+id/viewBr"
            android:layout_width="0.5dp"
            android:layout_height="match_parent"
            android:background="@color/modify_order_border" />

        <LinearLayout
            android:id="@+id/br_layout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:padding="@dimen/_5sdp">

            <com.acumengroup.ui.textview.GreekTextView
                android:id="@+id/txt_inv_FOQty"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Inv. Val"
                android:textColor="@color/white"
                android:textSize="@dimen/_10sdp" />

            <com.acumengroup.ui.textview.GreekTextView
                android:id="@+id/txt_mtfQty"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Avg. Price"
                android:textColor="@color/white"
                android:textSize="@dimen/_9sdp" />


        </LinearLayout>


    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/modify_order_border" />


</LinearLayout>
