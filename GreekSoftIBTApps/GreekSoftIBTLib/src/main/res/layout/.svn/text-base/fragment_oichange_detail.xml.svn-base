<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/ll_table"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:orientation="vertical">




        <LinearLayout
            android:id="@+id/header_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#000"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/one_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:weightSum="3"

                android:orientation="horizontal">

                <Spinner
                    android:id="@+id/spnr_exchange"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:padding="5dp"
                    >

                </Spinner>
                <Spinner
                    android:id="@+id/spnr_symbol"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:padding="5dp"
                    >

                </Spinner>

                <Spinner
                    android:id="@+id/spnr_expiry"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:padding="5dp"
                    >

                </Spinner>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                >
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="10dp"
                    >


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="20dp"
                        android:orientation="horizontal"
                        android:gravity="center_vertical"
                        android:weightSum="2"
                        >
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="2dp"
                            android:background="@color/white"
                            android:layout_weight="1"/>

                        <com.acumengroup.ui.textview.GreekTextView
                            android:id="@+id/ltpText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="bottom"
                            android:text="Strike Range"
                            android:textColor="@color/white"
                            android:textSize="15sp" />

                        <View
                            android:layout_width="wrap_content"
                            android:layout_height="2dp"
                            android:background="@color/white"
                            android:layout_marginRight="5dp"/>

                    </LinearLayout>
                    <!--
                                        <bfsl.GreekTextView
                                            android:id="@+id/ltpText"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_margin="5dp"
                                            android:gravity="center"
                                            android:text="Strike Range"
                                            android:textColor="@color/black"
                                            android:textSize="15sp" />-->
                    <LinearLayout
                        android:id="@+id/two_layout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:gravity="center"
                        android:padding="12dp"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/topless_boarder"
                        android:weightSum="2">
                        <Spinner
                            android:id="@+id/spnr_fromStrike"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:layout_margin="5dp"
                            android:padding="5dp"
                            />

                        <Spinner
                            android:id="@+id/spnr_toStrike"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:layout_margin="5dp"
                            android:padding="5dp"
                            />

                    </LinearLayout>
                </RelativeLayout>
            </LinearLayout>
            <com.acumengroup.ui.button.GreekButton
                android:id="@+id/btn_scan"
                android:layout_gravity="center"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:gravity="center"
                android:text="Filter"
                android:textColor="@color/white"
                android:textSize="12sp"
                android:visibility="gone"/>





        </LinearLayout>
    </LinearLayout>
    <!--    <LinearLayout
        android:id="@+id/ll_table"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:orientation="horizontal"
        android:weightSum="7">-->
    <com.acumengroup.ui.textview.GreekTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/bajaj_black"
        android:textSize="12sp"
        android:paddingLeft="10dp"
        android:textColor="@color/white"
        android:text="*All OI figures are in lakhs"/>


    <LinearLayout
        android:id="@+id/header_layout_interest"
        android:layout_width="match_parent"
        android:layout_height="25dp"
        android:background="@color/blue_background"
        android:orientation="horizontal"
        >

        <com.acumengroup.ui.textview.GreekTextView
            android:id="@+id/symbolText"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="2dp"
            android:layout_weight="1.6"
            android:gravity="right|center"
            android:paddingLeft="2dp"
            android:text="Strike"
            android:textColor="@color/white"

            android:textSize="10sp" />


        <com.acumengroup.ui.textview.GreekTextView
            android:id="@+id/callText"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="2dp"
            android:layout_weight="1"

            android:text="Call\nPut"
            android:textColor="@color/white"
            android:gravity="right|center"

            android:textSize="10sp" />

        <com.acumengroup.ui.textview.GreekTextView
            android:id="@+id/perchangeText"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="2dp"
            android:layout_weight="1"
            android:text="LTP"
            android:textColor="@color/white"
            android:gravity="right|center"
            android:textSize="9.5sp" />

        <com.acumengroup.ui.textview.GreekTextView
            android:id="@+id/mwplpertod"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="2dp"
            android:paddingLeft="2dp"
            android:layout_weight="1.2"
            android:gravity="right|center"
            android:text="chg"
            android:textColor="@color/white"
            android:textSize="10sp" />

        <!--  <bfsl.GreekTextView
            android:id="@+id/mwplperprev"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:layout_weight="1"
            android:gravity="center"
            android:text="IV"
            android:textColor="@color/black"
            android:textSize="12sp" />-->
        <com.acumengroup.ui.textview.GreekTextView
            android:id="@+id/mwplperprev"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="2dp"
            android:layout_weight="1.1"
            android:gravity="center"
            android:text="IV"
            android:textColor="@color/white"
            android:textSize="10sp" />

        <com.acumengroup.ui.textview.GreekTextView
            android:id="@+id/txtltp"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="2dp"
            android:layout_weight="1.2"
            android:gravity="center"
            android:text="OI"
            android:textColor="@color/white"
            android:textSize="10sp" />


        <com.acumengroup.ui.textview.GreekTextView
            android:id="@+id/txtbidqty"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="2dp"
            android:layout_weight="1.2"
            android:gravity="center"
            android:text="chng%"
            android:textColor="@color/white"
            android:textSize="10sp" />

        <com.acumengroup.ui.textview.GreekTextView
            android:id="@+id/txtbidprice"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="2dp"
            android:layout_weight="1.2"
            android:gravity="center"
            android:text="Vol."
            android:textColor="@color/white"
            android:textSize="10sp" />

        <com.acumengroup.ui.textview.GreekTextView
            android:id="@+id/txtaskqty"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:layout_weight="1.2"
            android:gravity="right"
            android:text="LTP"
            android:textColor="@color/black"
            android:textSize="10sp"
            android:visibility="gone"/>

        <com.acumengroup.ui.textview.GreekTextView
            android:id="@+id/txtaskprice"
            android:layout_width="0dp"
            android:layout_height="6dp"
            android:layout_margin="2dp"
            android:layout_weight="1.2"
            android:gravity="right"
            android:text="LTP"
            android:textColor="@color/black"
            android:textSize="12sp"
            android:visibility="gone"/>





        <!--<bfsl.GreekTextView
        android:id="@+id/mwplperprev"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_margin="2dp"
        android:layout_weight="1"
        android:gravity="center"
        android:text="Theta"
        android:textColor="@color/black"
        android:textSize="12sp" />
        <bfsl.GreekTextView
        android:id="@+id/mwplperprev"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_margin="2dp"
        android:layout_weight="1"
        android:gravity="center"
        android:text="Vega"
        android:textColor="@color/black"
        android:textSize="12sp" />-->


    </LinearLayout>
    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/oi_detail_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="@android:color/black"
        android:dividerHeight="0.5dip">

    </androidx.recyclerview.widget.RecyclerView>


</LinearLayout>