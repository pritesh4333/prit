                    android:checked="false"
                    android:gravity="center"
                    android:padding="@dimen/_5sdp" />


                <com.acumengroup.ui.button.GreekButton
                    android:id="@+id/btn_submit"
                    android:layout_width="@dimen/_150sdp"
                    android:layout_height="wrap_content"
