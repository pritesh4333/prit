    public void fromJSONObject(JSONObject data) {
        this.uniqueId = data.optString("UniqueId");
        this.priceunit = data.optString("priceunit");
        this.detailName = data.optString("detailName");
        this.instrumentName = data.optString("instrumentName");
        this.scriptName = data.optString("ScriptName");
        this.series = data.optString("Series");
        this.exchange = data.optString("exchange");
        this.assetType = data.optString("assetType");
        this.token = data.optString("token");

        if (!data.optString("expiryDate").isEmpty()) {

            this.expiryTimeStamp = data.optString("expiryDate");
            if (data.optString("expiryDate").equalsIgnoreCase("-19800")) {
                this.expiryDate = data.optString("expiryDate");
            } else {
                this.expiryDate = DateTimeFormatter.getDateFromTimeStamp(data.optString("expiryDate"), "dd MMM yyyy", "bse");
            }
        }

        if (!data.optString("expiry").isEmpty() ) {

            this.expiryTimeStamp = data.optString("expiry");

            if (data.optString("expiry").equalsIgnoreCase("-19800")) {
                this.expiry = data.optString("expiry");
            } else {
                this.expiry = DateTimeFormatter.getDateFromTimeStamp(data.optString("expiry"), "dd MMM yyyy", "bse");
            }

        }


        if (data.optString("strickPrice").length() > 0 && data.optString("assetType").equalsIgnoreCase("currency")) {
            double temp = data.optDouble("strickPrice");
            this.strickPrice = String.format("%.4f", temp);
        } else if (data.optString("strickPrice").length() > 0 && !data.optString("assetType").equalsIgnoreCase("currency")) {
            double temp = data.optDouble("strickPrice");
            this.strickPrice = String.format("%.2f", temp);
        } else {
            this.strickPrice = data.optString("strickPrice");
        }
        this.optionType = data.optString("OptionType");
        this.name = data.optString("Name");

        this.tradeSymbol = data.optString("tradeSymbol");
        this.description = data.optString("description");
        this.assetType = data.optString("assetType");
        this.lotQty = data.optString("lotQty");
        this.greekView = data.optString("greekView");
        this.multiplier = data.optString("multiplier");
        this.normalMarketEligibility = data.optString("normalMarketEligibility");
        this.tickSize = data.optString("tickSize");
        this.yHigh = data.optString("yHigh");
        this.yLow = data.optString("yLow");
        this.bseRTACode = data.optString("bseRTACode");
        this.bseCode = data.optString("bseCode");
    }

