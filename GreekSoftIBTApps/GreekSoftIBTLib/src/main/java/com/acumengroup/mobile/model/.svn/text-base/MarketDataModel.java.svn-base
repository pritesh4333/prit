    public JSONObject toJSON() throws JSONException {
        JSONObject jo = new JSONObject();
        jo.put("assetType", this.assetType);
        jo.put("type", this.type);
        jo.put("exchange", this.exchange);
        jo.put("token", this.token);
        jo.put("companyName", this.companyName);
        jo.put("expiryDate", this.expiryDate);
        jo.put("symbol", this.symbol);
        jo.put("ltp", this.ltp);
        jo.put("change", this.change);
        jo.put("perChange", this.perChange);
        jo.put("vol_traded", this.volume);
        jo.put("value", this.value);
        jo.put("oi", this.oi);
        jo.put("InstName", this.InstName);
        jo.put("StrikePrice", this.StrikePrice);
        jo.put("OptType", this.OptType);
        jo.put("instrumentname", this.instrumentname);
        jo.put("strikeprice", this.strikeprice);
        jo.put("optiontype", this.optiontype);
        jo.put("description", this.description);
        jo.put("cashInstName", this.cashInstName);
        jo.put("tickSize", this.tickSize);
        jo.put("multiply_factor", this.multiply_factor);
        jo.put("lotSize", this.lotSize);


        jo.put("schemeName", this.schemeName);
        jo.put("schemeCode", this.schemeCode);
        jo.put("navValue", this.navValue);
        jo.put("navDate", this.navDate);
        jo.put("tradingISIN", this.tradingISIN);
        jo.put("sipISIN", this.sipISIN);
        jo.put("oneYearRet", this.oneYearRet);
        jo.put("threeYearRet", this.threeYearRet);
        jo.put("fiveYearRet", this.fiveYearRet);
        jo.put("incRet", this.incRet);
        jo.put("Ltp1", this.Ltp1);
        jo.put("Ltp2", this.Ltp2);
        jo.put("token1", this.token1);
        jo.put("token2", this.token2);

        return jo;
    }

    public void fromJSON(JSONObject jo) throws JSONException {
        this.assetType = jo.optString("assetType");
        this.type = jo.optString("type");
        this.exchange = jo.optString("exchange");
        this.token = jo.optString("token");
        this.companyName = jo.optString("companyName");
        this.expiryDate = jo.optString("expiryDate");
       /* if (this.expiryDate.length() == 13) {
            this.expiryDate = DateTimeFormatter.getDateFromTimeStamp(this.expiryDate, "dd MMM yyyy", "bse");
        }*/
        this.symbol = jo.optString("symbol");
        this.ltp = jo.optString("ltp");
        this.change = jo.optString("change");
        this.perChange = jo.optString("perChange");
        if (jo.optString("vol_traded").length() > 0) {
            Double vol = Double.valueOf(jo.optString("vol_traded")) / 1000;
            this.volume = StringStuff.commaINRDecorator(jo.optString("vol_traded"));
        } else {
            this.volume = jo.optString("vol_traded");
        }
        if (jo.optString("value").length() > 0) {
            Double vol = Double.valueOf(jo.optString("value")) / 100000;
            this.value = String.valueOf(vol);
        } else {
            this.value = jo.optString("value");
        }
        this.oi = jo.optString("oi");
        this.InstName = jo.optString("InstName");
        this.StrikePrice = jo.optString("StrikePrice");
        this.OptType = jo.optString("OptType");
        this.instrumentname = jo.optString("instrumentname");
        this.strikeprice = jo.optString("strikeprice");
        this.optiontype = jo.optString("optiontype");
        this.description = jo.optString("description");
        this.cashInstName = jo.optString("cashInstName");
        this.tickSize = jo.optString("tickSize");
        this.multiply_factor = jo.optString("multiply_factor");
        this.lotSize = jo.optString("lotSize");

        this.schemeName = jo.optString("schemeName");
        this.schemeCode = jo.optString("schemeCode");
        this.navValue = jo.optString("navValue");
        this.navDate = jo.optString("navDate");
        this.tradingISIN = jo.optString("tradingISIN");
        this.sipISIN = jo.optString("sipISIN");
        this.oneYearRet = jo.optString("oneYearRet");
        this.threeYearRet = jo.optString("threeYearRet");
        this.fiveYearRet = jo.optString("fiveYearRet");
        this.incRet = jo.optString("incRet");

        this.Ltp1 = jo.optString("Ltp1");
        this.Ltp2 = jo.optString("Ltp2");
        this.token1 = jo.optString("token1");
        this.token2 = jo.optString("token2");
    }

